@model ContaOnline.Domain.Models.ContaCategoria

@{
    ViewBag.Title = "Excluir";
}

<h3>Excluir Categoria de Conta</h3>

<h4>Confirma a exclusão desse registro?</h4>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Descrição
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Nome)
        </dd>
    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Excluir" class="btn btn-danger" /> |
            <a href='@Url.Action("Inicio")' class="btn btn-default"> Voltar</a>
        </div>
    }
</div>
